Algoritmo "CONSUMO"

Var

   DISTANCIA, COMBUSTIVEL, MEDIA: REAL

Inicio

      ESCREVAL("Distancia percorrida: ")
      LEIA(DISTANCIA)
      ESCREVAL("Combust√≠vel gasto: ")
      LEIA(COMBUSTIVEL)
      
      
      MEDIA <- DISTANCIA / COMBUSTIVEL

      ESCREVAL
      ESCREVAL("Consumo medio = ", MEDIA:2:3)



